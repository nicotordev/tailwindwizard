{
  "name": "tw-wizard-api",
  "version": "1.0.0",
  "description": "TailwindWizard API for onboarding and resume parsing",
  "main": "src/server.ts",
  "type": "module",
  "private": true,
  "author": "Nicolas Torres < nicotordev@gmail.com > (https://nicotordev.com)",
  "homepage": "https://tailwindwizard.com",
  "scripts": {
    "dev": "bun --watch src/server.ts",
    "start": "bun src/server.ts",
    "build": "tsc && bun build src/server.ts --outdir ./dist --target bun",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "format": "prettier --write src",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio"
  },
  "license": "ISC",
  "packageManager": "bun@1.3.2",
  "dependencies": {
    "@clerk/backend": "^2.29.1",
    "@hono-rate-limiter/cloudflare": "^0.2.2",
    "@hono/clerk-auth": "^3.0.3",
    "@hono/swagger-ui": "^0.5.3",
    "@hono/zod-openapi": "^1.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "dotenv": "^17.2.3",
    "hono": "^4.6.14",
    "hono-pino": "^0.10.3",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "stripe": "^20.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@hono/eslint-config": "^2.0.4",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prisma": "^7.2.0",
    "typescript": "^5.7.2"
  }
}
