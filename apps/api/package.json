{
  "name": "tw-wizard-api",
  "version": "1.0.0",
  "description": "TailwindWizard API for onboarding and resume parsing",
  "main": "src/server.ts",
  "type": "module",
  "private": true,
  "author": "Nicolas Torres < nicotordev@gmail.com > (https://nicotordev.com)",
  "homepage": "https://tailwindwizard.com",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "prebuild": "pnpm --filter @tw/shared build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "postinstall": "pnpm run db:generate"
  },
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.966.0",
    "@aws-sdk/lib-storage": "^3.966.0",
    "@clerk/backend": "^2.29.1",
    "@hono-rate-limiter/cloudflare": "^0.2.2",
    "@hono/clerk-auth": "^3.0.3",
    "@hono/node-server": "^1.19.7",
    "@hono/swagger-ui": "^0.5.3",
    "@hono/zod-openapi": "0.16.4",
    "@onesignal/node-onesignal": "5.3.1-beta1",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@tw/shared": "workspace:*",
    "@types/acorn": "^6.0.4",
    "@types/adm-zip": "^0.5.7",
    "acorn": "^8.15.0",
    "acorn-jsx": "^5.3.2",
    "adm-zip": "^0.5.16",
    "dotenv": "^17.2.3",
    "hono": "4.11.3",
    "hono-pino": "^0.10.3",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "playwright": "^1.57.0",
    "sharp": "^0.34.5",
    "stripe": "^20.1.2",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@hono/eslint-config": "^2.0.4",
    "@types/estree": "^1.0.8",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "eslint": "^9.17.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.4.2",
    "prisma": "^7.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
